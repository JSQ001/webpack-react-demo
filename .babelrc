{
  "presets": [
    [
      "es2015", { "modules": false }
    ]
    //"react",
    //"airbnb"
  ],
  "plugins": [
    "transform-decorators-legacy",  //babel7中是默认插件，类装饰器（注解）
    "transform-object-rest-spread",
    "react-hot-loader/babel"
  ],
  "env": {
    "test": {
      "plugins": [
        "transform-decorators-legacy",
        "transform-object-rest-spread",
        "istanbul"
      ]
    }
  }
}
